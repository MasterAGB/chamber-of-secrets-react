{
  "expo": {
    "name": "chamber-of-secrets",
    "slug": "chamber-of-secrets",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "githubUrl": "https://github.com/MasterAGB/chamber-of-secrets-react",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "This app needs access to the photo library to save files."
      },
      "supportsTablet": true
    },
    "android": {
      "permissions": ["WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.masteragb.chamberofsecrets"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ed88f7ce-f9ac-4587-a9c4-13b147677e4e"
      }
    },
    "owner": "masteragb"
  }
}
